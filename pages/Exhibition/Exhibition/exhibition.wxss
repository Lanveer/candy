  /* background: yellow; */
  display: flex;
  flex-direction: row;
  align-items: center;
  vertical-align: middle;
}

.locationImage {
  width: 20rpx;
  height: 20rpx;
}

.locationFont {
  font-size: 9pt;
  color: #226298;
  margin-left: 10rpx;
}

