  top: 26rpx;
}

.hotelDescriptionView {
  width: 498rpx;
  height: 200rpx;
  margin: auto;
  text-align: left;
  margin-top: 5rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  line-height: 90%;
}

.descriptionFont {
  font-size: 26rpx;
  color: #353535;
}

.loactionView {
  width: 276rpx;
  height: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
}

.locationContainer {
  width: 90%;
