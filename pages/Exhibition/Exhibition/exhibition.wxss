/* pages/Exhibition/Exhibition/exhibition.wxss */
@import "../../template_regist/regist.wxss";

.container {
  width: 750rpx;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
}

/*搜索栏*/

.searchView {
  width: 358rpx;
  height: 70rpx;
  border-radius: 40rpx;
  border-color: #ffe8e6;
  border: 2rpx solid #ffe8e6;
  background: #fff;
  margin-top: -30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  z-index: 99;
}

.searchContainer {
  width: 260rpx;
  height: 70rpx;
  background: #fff;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.searchImage {
  width: 44rpx;
  height: 44rpx;
}

.searchTitle {
  font-size: 24rpx;
  color: #999;
}

/*Banner*/

.banner {
  width: 750rpx;
  height: 392rpx;
}

.banner swiper {
  height: 392rpx;
}

.banner image {
  width: 750rpx;
  height: 100%;
}

/*顶部分类信息栏*/

.headerContainerView {
  width: 750rpx;
  height: 170rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-content: center;
  margin-bottom: 10rpx;
}

/*顶部分类信息栏容器*/

.headerView {
  width: 720rpx;
  height: 170rpx;
  display: flex;
  flex-direction: row;
  /*换行*/
  flex-wrap: wrap;
  align-items: center;
  margin: auto;
}

/*分类信息cell*/

.cell {
  width: 144rpx;
  height: 130rpx;
  flex-direction: column;
  align-items: center;
  text-align: center;
  /* background: red; */
}

/*分类信息图片*/

.headerIconImage {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  align-items: center;
  margin-top: 10rpx;
}

/*分类信息title*/

.headerIconText {
  font-size: 24rpx;
  display: block;
  margin-top: -5rpx;
}

/*酒店 | 展馆 按钮*/

.middleTapBar {
  width: 750rpx;
  height: 20rpx;
  background: #f1f1f1;
  display: flex;
  flex-direction: row;
}

.middleBarItem {
  width: 475rpx;
  height: 66rpx;
  background: #f1f1f1;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.middleBarTitle {
  font-size: 30rpx;
  color: #666;
  margin: auto;
  text-align: center;
  line-height: 60rpx;
}

.middleBarTitleSelected {
  font-size: 30rpx;
  color: #e82929;
}

.middleBarItemLine {
  background: #f1f1f1;
  height: 5rpx;
  width: 162rpx;
}

.middleBarItemLineSelected {
  background: #e82929;
  height: 5rpx;
  width: 162rpx;
}

.hotelFrontTitleContainer {
  width: 750rpx;
  height: 88rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.hotelFrontTitleView {
  width: 400rpx;
  height: 88rpx;
  /* background: #f1f1f1; */
  display: flex;
  flex-direction: row;
  align-content: center;
}

.hotelFrontTitleLine {
  width: 70rpx;
  height: 2rpx;
  background: #999;
  margin-top: 44rpx;
}

.hotelFrontTitleLineLeft {
  width: 70rpx;
  height: 2rpx;
  background: #999;
  margin-top: 44rpx;
  margin-left: 40rpx;
}

.hotelFrontTitleText {
  font-size: 32rpx;
  color: #999;
  margin-top: 24rpx;
  margin-left: 24rpx;
  margin-right: 24rpx;
}

/*test  */

.test {
  width: 100%;
  height: 150px;
  background: #ccc;
  white-space: nowrap;
}

.tags {
  width: 150px;
  height: 75px;
  background: green;
  margin-right: 10px;
  margin-top: 36px;
  display: inline-block;
}

