.section:first-child {
  border-top: 1rpx solid #ddd;
}

.section {
  width: 100%;
  height: 35pt;
  border-bottom: 1rpx solid #ddd;
  display: flex;
  flex-direction: row;
  align-items: center;
  vertical-align: middle;
  font-size: 10pt;
  color: #333;
}

.left {
  width: 200rpx;
  margin-left: 10pt;
}
